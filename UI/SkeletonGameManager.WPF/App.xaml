<Application x:Class="SkeletonGameManager.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:properties="clr-namespace:SkeletonGameManager.Resources.Properties;assembly=SkeletonGameManager.Resources">
    <Application.Resources>
        
        <ResourceDictionary>

            <!--Localization-->
            <properties:Resources x:Key="AppStringResource"/>

            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="pack://application:,,,/SkeletonGameManager.Resources;component/BrushResources.xaml" />
                <ResourceDictionary Source="pack://application:,,,/SkeletonGameManager.Resources;component/ImageResources.xaml" />
                <ResourceDictionary Source="/SkeletonGameManager.Resources;component/Style/SkelegameWpf.xaml" />
                <ResourceDictionary Source="pack://application:,,,/SkeletonGameManager.Resources;component/ControlResources.xaml" />
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>

    </Application.Resources>
</Application>
