<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <RadialGradientBrush x:Key="Brush_GradientBackground" GradientOrigin="0.5,0.5" SpreadMethod="Repeat" Opacity="0.51" RadiusX="56">
        <GradientStop Color="#FFCCDDE4"/>
        <GradientStop Color="#FFFDFDFD" Offset="1"/>
    </RadialGradientBrush>

    <SolidColorBrush x:Key="Brush_NotUsed" Color="#FF816B6B"/>
    <SolidColorBrush x:Key="Brush_Used" Color="#FF6D9B65"/>
    <SolidColorBrush x:Key="Brush_MatrixLabel" Color="Green"/>

</ResourceDictionary>